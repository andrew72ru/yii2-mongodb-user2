{
  "name": "andrew72ru/yii2-mongodb-user",
  "description": "User and Profile implementation for Yii2 with mongo db",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Zhdanovskih",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "^2.0",
    "intervention/image": "^2.3",
    "yiisoft/yii2-mongodb": "^2.1",
    "andrew72ru/yii2-flowjs": "*"
  },
  "autoload": {
    "psr-4": {
      "andrew72ru\\user\\": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/andrew72ru/yii2-flowjs"
    }
  ],
  "extra": {
    "bootstrap": "andrew72ru\\user\\Bootstrap",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "asset-vcs-driver-options": {
      "github-no-api": true
    },
    "asset-pattern-skip-version": "(-build)"
  }
}
